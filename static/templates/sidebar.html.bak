				<li {% if page = "periode" %} class="active" {% endif %}><a href="/penilaian/periode/"><i class="icon-chevron-right"></i>  Data Periode</a></li>
				<li {% if page = "nilai" %} class="active" {% endif %}><a href="/penilaian/nilai/"><i class="icon-chevron-right"></i>  Data Nilai Siswa</a></li>
				{%endif%}
				{% if request.session.hak_akses == "guru" %}
				<li {% if page = "rekap_nilai" %} class="active" {% endif %}><a href="/penilaian/rekap/"><i class="icon-chevron-right"></i>  Rekap Nilai</a></li>
				<li {% if page = "rekap_kompetensi" %} class="active" {% endif %}><a href="/penilaian/kompetensi/"><i class="icon-chevron-right"></i>  Rekap Kompetensi</a></li>
				{%endif%}
				{%if request.session.hak_akses !=  "siswa"%}
				<li {% if page = "rekap_us" %} class="active" {% endif %}><a href="/penilaian/us"><i class="icon-chevron-right"></i>  Data Nilai Ujian Sekolah</a></li>
				<li {% if page = "rekap_un" %} class="active" {% endif %}><a href="/penilaian/un"><i class="icon-chevron-right"></i>  Data Nilai Ujian Nasional</a></li>
				{%endif%}
			</ul>
		</li>
		{% endif %}
		{% if request.session.hak_akses == "siswa" %}
		<li {% if page = "nilaiku" %} class="panel active" {% else %}class="panel" {% endif %}>
			<a href="/penilaian/nilaiku" >
			<i class="icon-pencil"></i> Nilai Saya
			</a>                   
		</li>
		{% endif %}
		{% if request.session.hak_akses == "Admin" or request.session.hak_akses == "staf" %}
		<li {% if pagegroups = "absensi" %} class="panel active"{% else %}class="panel" {% endif %}>
			<a href="#" data-parent="#menu" data-toggle="collapse" class="accordion-toggle" data-target="#laporan-nav">
				<i class="icon-book"> </i> Laporan
				<span class="pull-right">
				<i class="icon-chevron-left"></i>
			</a>
			<ul {% if pagegroups = "laporan" %} class="in" {% else %}class="collapse" {% endif %} id="laporan-nav">
				<li {% if page = "absensi_siswa" %} class="active" {% endif %}><a href="/laporan/absensi/siswa"><i class="icon-chevron-right"></i>  Absensi Siswa</a></li>
				<li {% if page = "absensi_guru" %} class="active" {% endif %}><a href="/laporan/absensi/guru"><i class="icon-chevron-right"></i>  Absensi Guru</a></li>
				<li {% if page = "rapor" %} class="active" {% endif %}><a href="/laporan/rapor/"><i class="icon-chevron-right"></i>  Rapor</a></li>
				<li {% if page = "transkrip" %} class="active" {% endif %}><a href="/laporan/transkrip/"><i class="icon-chevron-right"></i>  Transkrip Nilai</a></li>
			</ul>
		</li>
		{% endif %}
		<!--li {% if page = "materi" %} class="panel active" {% else %}class="panel" {% endif %}>
			<a href="/materi/" >
			<i class="icon-rocket"></i> Download Materi
			</a>                   
		</li-->
		{% if request.session.hak_akses = "Admin" or request.session.hak_akses = "staf" or request.session.hak_akses = "kepsek" %}
		<li {% if page = "user" %} class="panel active" {% else %}class="panel" {% endif %}>
			<a href="/user/" >
			<i class="icon-lock"></i> Pengaturan User
			</a>                   
		</li>
		{% endif %}
	</ul>
